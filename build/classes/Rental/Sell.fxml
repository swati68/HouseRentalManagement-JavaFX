<AnchorPane id="AnchorPane" fx:id="sellPane" prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rental.SellController">
   <children>
      <BorderPane layoutX="11.0" layoutY="14.0" prefHeight="119.0" prefWidth="879.0">
         <center>
            <Label alignment="CENTER" prefHeight="114.0" prefWidth="866.0" text="HOUSE RENTING SYSTEM" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Bookman Old Style Bold Italic" size="37.0" />
               </font>
            </Label>
         </center>
      </BorderPane>
      <Separator layoutX="23.0" layoutY="146.0" prefHeight="3.0" prefWidth="854.0" />
      <Label alignment="CENTER" layoutX="350.0" layoutY="119.0" prefHeight="27.0" prefWidth="201.0" text="HOUSE DETAILS">
         <font>
            <Font name="Baskerville Old Face" size="23.0" />
         </font>
      </Label>
      <TextField fx:id="tf1" layoutX="363.0" layoutY="161.0" promptText="Enter Location">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="tf2" layoutX="363.0" layoutY="203.0" promptText="Dimensions">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>
      <Label layoutX="552.0" layoutY="210.0" prefHeight="16.0" prefWidth="56.0" text="Sq. Feet">
         <font>
            <Font name="Arial Italic" size="13.0" />
         </font>
      </Label>
      <TextField fx:id="tf3" layoutX="363.0" layoutY="248.0" promptText="No. of Rooms">
         <font>
            <Font name="System Italic" size="14.0" />
         </font>
      </TextField>
      <TextField fx:id="tf4" layoutX="363.0" layoutY="294.0" promptText="No. of Bathrooms">
         <font>
            <Font name="System Italic" size="14.0" />
