      </TextField>
      <Label layoutX="552.0" layoutY="445.0" text="Rupees/Month">
         <font>
            <Font name="Arial Italic" size="13.0" />
         </font>
      </Label>
      <Button fx:id="btn1" layoutX="393.0" layoutY="498.0" mnemonicParsing="false" onAction="#toList" text="List For Rent">
         <font>
            <Font name="Bodoni MT" size="16.0" />
         </font>
      </Button>
      <Button fx:id="btn2" layoutX="424.0" layoutY="544.0" mnemonicParsing="false" onAction="#Back" text="Back">
         <font>
            <Font name="Bodoni MT" size="15.0" />
         </font>
      </Button>
